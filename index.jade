//- 技能模板
mixin skill(content) 
    skill
        block
        .content=content
mixin skillUnlocking(content) 
    skill.unlocking
        i.icons
            block
            i.lock.icon
        .content=content
//- 作品模板
mixin works(img,header,description,link,comment) 
    a.ts.card(href=link)
        .image
            img(src=img)
        .content
            .header=header
            .description=description
            .comment=comment

doctype html
html
    head
        title Rex owo//
        meta(name='viewport',content='width=device-width, initial-scale=1, shrink-to-fit=no')
        meta(charset='utf-8')
        link(rel="icon" href="favicon.ico")
        include ./Jades/_tags.jade
        +social_tags('Leko','這裡是 Leko，某鬼島上的可愛(X)大學生。','https://rextw.com/img/og.webp','https://rextw.com/')
    body(onload='$("#contents").fadeIn("normal");')
        noscript#deferred-styles
            link(href='https://fonts.googleapis.com/css?family=Jura',rel='stylesheet',type="text/css")
            link(href='https://jakiestfu.github.io/Ripple.js/dist/ripple.min.css',rel='stylesheet',type="text/css")
            link(href='style.css',rel='stylesheet',type="text/css")
            link(href='https://cdnjs.cloudflare.com/ajax/libs/tocas-ui/2.3.3/tocas.css',rel='stylesheet',type="text/css")
        script(src='load-css.js', async="true")
        script(data-cfasync="true" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js")
        script(data-cfasync="true" src="https://cdnjs.cloudflare.com/ajax/libs/pace/1.0.2/pace.min.js", async="true")
        script(data-cfasync="true" src="https://jakiestfu.github.io/Ripple.js/dist/ripple.min.js", defer="true")
        script(data-cfasync="true" src='https://rawgit.com/rexx0520/RandomPic-1/master/randompicture2.js', defer="true")
        script(data-cfasync="true" src='https://cdnjs.cloudflare.com/ajax/libs/tocas-ui/2.3.3/tocas.js', async="true")
        script(data-cfasync="true" src="script.js", defer="true")
        #contents(style='display:none;')
            header.ts.fluid.big.slate
                .image
                    img.randomPic
                img(src='avatar.webp',id='avater')
                span.header Leko
                span.description 更加了解 Leko ️❤
                #iconDiv.ts.grid.stackable
                    .doubling.sixteen.centered.column.row
                        .column
                            a(href="https://www.facebook.com/TWRexTg.0")
                                i.big.facebook.square.icon
                        .column
                            a(href="https://t.me/joinchat/AAAAAEejHdwHDPairYRecA")
                                i.big.telegram.icon
                        .column
                            a(href="https://github.com/rexx0520")
                                i.big.github.icon
                        .column
                            a(href="https://blog.leko.moe")
                                i.big.wordpress.icon

            .ts.container
